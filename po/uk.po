msgid "fast"
msgstr "швидко"

#: ../data/messages:383 ../data/messages:391 ../data/messages:395
#: ../data/messages:403 ../data/messages:407
msgid "slow"
msgstr "повільно"

#: ../data/messages:385 ../data/messages:397
